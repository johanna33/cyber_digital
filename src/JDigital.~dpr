program JDigital;

uses
  Forms,
  Unit1 in 'Unit1.pas' {fCaja},
  Unit3Login in 'Unit3Login.pas' {flogin},
  Unit2Menu in 'Unit2Menu.pas' {fMenu},
  Unit2Calculo in 'Unit2Calculo.pas' {fCalculo},
  Unit2Cuenta in 'Unit2Cuenta.pas' {fCuenta},
  Unit2Inventario in 'Unit2Inventario.pas' {fInven},
  Unit2Cliente in 'Unit2Cliente.pas' {fCliente},
  Unit2Empleado in 'Unit2Empleado.pas' {fEmpleado},
  Unit2Archivo in 'Unit2Archivo.pas' {fArchivo},
  Unit2Suplidor in 'Unit2Suplidor.pas' {fSupli};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TfMenu, fMenu);
  Application.CreateForm(TfCaja, fCaja);
  Application.CreateForm(Tflogin, flogin);
  Application.CreateForm(TfCalculo, fCalculo);
  Application.CreateForm(TfCuenta, fCuenta);
  Application.CreateForm(TfInven, fInven);
  Application.CreateForm(TfCliente, fCliente);
  Application.CreateForm(TfEmpleado, fEmpleado);
  Application.CreateForm(TfArchivo, fArchivo);
  Application.CreateForm(TfSupli, fSupli);
  Application.Run;
